  :dev
  {:extra-deps {ring/ring-devel {:mvn/version "1.9.6"}
                io.github.nubank/morse {:git/tag "v2023.04.27.01" :git/sha "7a85e46"}
                org.clojure/tools.trace {:mvn/version "0.7.11"}}}}}
